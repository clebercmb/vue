e.log(sessionStorage)\n    this.userName = sessionStorage.getItem('username')\n    // console.log(this.userName)\n  }\n}\n</script>\n\n<style>\n@import '../node_modules/bootstrap/dist/css/bootstrap.min.css';\n.iconpos{\n    position: relative;\n    top: 10px;\n}\n\n.footertxtpos{\n    position: relative;\n    top: 20px;\n    left: 520px;\n}\n</style>\n"]}]}